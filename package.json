{
  "name": "lambda-scraper-queue",
  "version": "0.0.1",
  "description": "Web Scraper Queue built with AWS Lambda",
  "main": "config.js",
  "scripts": {
    "compile-lambda": "rm -rf ./build/apex/functions && ./node_modules/.bin/webpack --config deploy/apex/webpack.config.js --display-exclude --display-modules --display-chunks --display-error-details --display-origins --display-cached --display-cached-assets",
    "deploy-lambda": "cd build/apex && apex deploy"
  },
  "author": "Jim Pick <jim@jimpick.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "string-replace-webpack-plugin": "0.0.3",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "aws-sdk": "^2.2.38"
  }
}
